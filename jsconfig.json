{
	"compilerOptions": {
	  "baseUrl": ".",
	  "esModuleInterop": true,
	  "skipLibCheck": true,
	  "forceConsistentCasingInFileNames": true,
	  "module": "commonjs",
	  "moduleResolution": "node",
	  "resolveJsonModule": true,
	  "isolatedModules": true,
	  "incremental": true,
	  "lib": [
		"dom",
		"esnext"
	  ],
	  "strict": true,
	  "target": "esnext"
	},
	"include": [
	  "**/*.js",
	  "**/*.jsx"
	],
	"exclude": ["node_modules"]
  }
  